{
  "name": "rintisch/tailwindcss-form",
  "type": "typo3-cms-extension",
  "version": "1.3.0",
  "description": "Adapt output of EXT:form for usage with tailwindcs",
  "homepage": "https://github.com/rintisch/",
  "license": [
    "GPL-2.0-or-later"
  ],
  "keywords": [
    "TYPO3 CMS"
  ],
  "require": {
    "typo3/cms-core": "^10.4 || ^11.5",
    "typo3/cms-form": "^10.4 || ^11.5"
  },
  "autoload": {
    "psr-4": {
      "Rintisch\\TailwindcssForm\\": "Classes/"
    }
  },
  "extra": {
    "typo3/cms": {
      "extension-key": "tailwindcss_form"
    }
  }
}
